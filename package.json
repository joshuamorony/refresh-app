{
  "name": "refresh-app",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "https://ionicframework.com/",
  "scripts": {
    "ng": "ng",
    "start": "firebase emulators:exec --project=demo-project --ui --import=./firebase-data 'ng serve'",
    "build": "ng build",
    "test": "ng test --watch-all",
    "test:ci": "ng test",
    "test:rules": "firebase emulators:exec 'jest test --config=firebase.jest.config.js --detectOpenHandles --forceExit'",
    "test:rules:ci": "lsof -t -i:4400 -i:8080 -i:9099 | xargs kill -9 && npm run test:rules",
    "lint": "ng lint",
    "e2e": "cross-env FIREBASE_AUTH_EMULATOR_HOST=9099 FIRESTORE_EMULATOR_HOST=8080 firebase emulators:exec --project=demo-project --ui --import=./firebase-data 'ng e2e'",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~13.0.0",
    "@angular/core": "~13.0.0",
    "@angular/fire": "^7.2.1",
    "@angular/forms": "~13.0.0",
    "@angular/platform-browser": "~13.0.0",
    "@angular/platform-browser-dynamic": "~13.0.0",
    "@angular/router": "~13.0.0",
    "@angular/service-worker": "~13.0.0",
    "@capacitor/app": "1.1.0",
    "@capacitor/core": "3.4.1",
    "@capacitor/haptics": "1.1.4",
    "@capacitor/keyboard": "1.2.2",
    "@capacitor/status-bar": "1.0.8",
    "@firebase/rules-unit-testing": "^2.0.2",
    "@ionic/angular": "^6.0.12",
    "@ngrx/component-store": "^13.0.2",
    "firebase": "^9.6.7",
    "rxfire": "^6.0.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "latest",
    "@angular-devkit/build-angular": "~13.0.1",
    "@angular-eslint/builder": "~13.0.1",
    "@angular-eslint/eslint-plugin": "~13.0.1",
    "@angular-eslint/eslint-plugin-template": "~13.0.1",
    "@angular-eslint/template-parser": "~13.0.1",
    "@angular/cli": "~13.0.1",
    "@angular/compiler": "~13.0.0",
    "@angular/compiler-cli": "~13.0.0",
    "@angular/language-service": "~13.0.0",
    "@briebug/jest-schematic": "^4.0.0",
    "@capacitor/cli": "3.4.1",
    "@cypress/schematic": "^1.6.0",
    "@hirez_io/observer-spy": "^2.1.2",
    "@ionic/angular-toolkit": "^5.0.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "latest",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "cross-env": "^7.0.3",
    "cypress": "^10.1.0",
    "cypress-firebase": "^2.2.1",
    "cypress-log-to-output": "^1.1.2",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "firebase-admin": "^9.12.0",
    "jest": "latest",
    "ts-node": "~8.3.0",
    "typescript": "~4.4.4"
  },
  "description": "An Ionic project"
